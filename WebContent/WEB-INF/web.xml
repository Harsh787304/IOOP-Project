<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                             http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <!-- Servlet registration for AddResidentServlet -->
    <servlet>
        <servlet-name>AddResidentServlet</servlet-name>
        <servlet-class>AddResidentServlet</servlet-class>
    </servlet>

    <!-- Servlet mapping for AddResidentServlet -->
    <servlet-mapping>
        <servlet-name>AddResidentServlet</servlet-name>
        <url-pattern>/AddResidentServlet</url-pattern>
    </servlet-mapping>

</web-app>
